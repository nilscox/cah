{
  "presets": ["babel-preset-expo"],
  "env": {
    "development": {
      "plugins": ["transform-react-jsx-source"]
    }
  },
  "plugins": [
    "transform-inline-environment-variables",
    [
      "babel-plugin-root-import", [{
        "rootPathPrefix": "~",
        "rootPathSuffix": "src"
      }]
    ]
  ]
}
